<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/consultart_user_management/stylesheets' %>
    <%= javascript_include_tag "consultart_user_management/application" %>
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/consultart_user_management/header' %>
      <section class="round">
      <% flash.each do |key, value| %>
        <%= content_tag(:div, value, :class => "flash #{key}") %>
      <% end %>
      <%= yield %>
      </section>
      <%= debug(params) if Rails.env.development? || Rails.env.test? %>
    </div>
  </body>
</html>



