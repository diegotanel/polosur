<h1><%=t("activerecord.models.tipo_de_gasto") %></h1>

<table>
  <tr>
    <th><%=t("activerecord.attributes.tipo_de_gasto.detalle") %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <%= content_tag_for(:tr, @tipos_de_gasto) do |tipo_de_gasto| %>
    <td><%= tipo_de_gasto.detalle %></td>
    <td><%= link_to t("scaffoldi18n.show"), tipo_de_gasto %></td>
    <td><%= link_to t("scaffoldi18n.edit"), edit_tipo_de_gasto_path(tipo_de_gasto) %></td>
    <td><%= link_to t("scaffoldi18n.destroy"), tipo_de_gasto, confirm: t("scaffoldi18n.are_you_sure?"), method: :delete %></td>
  <% end %>
</table>

<br />

<%= link_to t('scaffoldi18n.new'), new_tipo_de_gasto_path %>


